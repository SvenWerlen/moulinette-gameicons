{
  "name": "moulinette-gameicons",
  "title": "Moulinette Forge Game Icons (module)",
  "description": "This module for Moulinette adds capabilities for searching and downloading icons from game-icons.net.",
  "version": "2.0.2",
  "author": "Sven Werlen",
  "esmodules": ["moulinette-gameicons.js"],
  "styles": ["moulinette-gameicons.css"],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    },
    {
      "lang": "fr",
      "name": "French",
      "path": "lang/fr.json"
    }
  ],
  "dependencies": [
    {
      "name": "moulinette-core",
      "type": "module"
    }
  ],
  "url": "https://github.com/SvenWerlen/moulinette-gameicons",
  "download": "https://github.com/SvenWerlen/moulinette-gameicons/archive/v-2.0.2.zip",
  "manifest": "https://raw.githubusercontent.com/SvenWerlen/moulinette-gameicons/master/module.json",
  "minimumCoreVersion": "0.7.6",
  "compatibleCoreVersion": "0.8.6"
}
